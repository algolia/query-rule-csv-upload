{
  "name": "query-rule-upload",
  "version": "1.0.0",
  "description": "A tool to upload query rules from CSV",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder -mw"
  },
  "dependencies": {
    "algoliasearch": "^3.35.1",
    "csv-parser": "^2.3.2",
    "csv-string": "^3.2.0",
    "moment": "^2.25.3"
  },
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^22.6.0"
  },
  "build": {
    "appId": "com.algolia.queryruleupload",
    "productName": "Query Rule Upload",
    "artifactName": "${productName}-${os}.${ext}"
  }
}
